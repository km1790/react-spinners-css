{
    "printWidth": 100,
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "overrides": [
        {
            "files": ["*.rt", "*.rts"],
            "options": {
                "parser": "html",
                "htmlWhitespaceSensitivity": "ignore"
            }
        }
    ]
